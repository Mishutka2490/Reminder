<?xml version="1.0"?>
    <project name="Reminder" default="build" basedir=".">
        <description>The assembling info file for Reminder project.</description>
        <property name="debug" value="true" overwrite="false" />
        <target name="build" description="compiles the source code">
            <csc target="exe" output="Reminder.exe">
                <sources>
                    <include name="**/*.cs"/>
                </sources>
            </csc>
        </target>
    </project>